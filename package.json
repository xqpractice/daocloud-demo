{
  "name": "daocloud-demo",
  "version": "0.0.2",
  "description": "A demo for daocloud",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "start": "NODE_ENV=production node config/server.js",
    "start:dev": "NODE_ENV=developer node config/server.dev.js",
    "lint": "eslint src",
    "clean": "rm -rf ./lib ./public/assets",
    "babel": "babel -d lib src --copy-files",
    "babel:watch": "watch 'yarn run babel' ./src",
    "build": "yarn run clean && yarn run babel && webpack --config config/webpack/webpack.config.js",
    "test:unit": "mocha --compilers js:babel-register test/SpecHelper.js test/js/*-spec.js test/js/**/*-spec.js",
    "test": "yarn install && yarn run lint && yarn run test:unit",
    "test:watch": "watch 'yarn run test' ./src ./test"
  },
  "dependencies": {
    "common-basic-validator": "^0.3.2",
    "express": "^4.14.0",
    "express-common-controller": "^3.0.0",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "promise-polyfill": "^6.0.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "simple-string-template": "^0.1.1",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "node-sass": "^3.10.1",
    "null-loader": "^0.1.1",
    "react-addons-test-utils": "^15.4.2",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "^1.0.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-glob-entry": "^1.2.7",
    "webpack-hot-middleware": "^2.12.2",
    "when": "^3.7.8"
  },
  "repository": "git@github.com:xqpractice/daocloud-demo.git",
  "author": "xqcao@thoughtworks.com",
  "license": "MIT"
}
